%
  ID
  NUM
  REGEXP
;

input = syntax*
      ;

syntax = ID '=' body_list ';'
       | '%' ID* ';'
       ;

body_list = body_internal* ('|' body_internal*)*
          ;

body_internal = ','? body
              ;

body = '(' body_list ')' body_opt*
     | ID body_opt*
     ;

body_opt = '?'
         | '+'
         | '*'
         | '{' NUM (',' NUM)? '}'
         ;

